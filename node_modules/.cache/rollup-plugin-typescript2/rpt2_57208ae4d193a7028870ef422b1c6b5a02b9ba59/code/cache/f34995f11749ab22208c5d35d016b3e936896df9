 },\r\n    // control snap position alignment\r\n    dir: {\r\n        default: defaultConfigs.dir,\r\n        validator(value) {\r\n            // The value must match one of these strings\r\n            return ['rtl', 'ltr'].includes(value);\r\n        },\r\n    },\r\n    // an object to pass all settings\r\n    settings: {\r\n        default() {\r\n            return {};\r\n        },\r\n        type: Object,\r\n    },\r\n};\r\n","references":["/Users/I560079/Documents/vue-carousel/src/partials/defaults.ts"],"dts":{"name":"/Users/I560079/Documents/vue-carousel/dist/dts/partials/props.d.ts","writeByteOrderMark":false,"text":"export declare const carouselProps: {\r\n    itemsToShow: {\r\n        default: number;\r\n        type: NumberConstructor;\r\n    };\r\n    itemsToScroll: {\r\n        default: number;\r\n        type: NumberConstructor;\r\n    };\r\n    wrapAround: {\r\n        default: boolean | undefined;\r\n        type: BooleanConstructor;\r\n    };\r\n    throttle: {\r\n        default: number | undefined;\r\n        type: NumberConstructor;\r\n    };\r\n    snapAlign: {\r\n        default: import(\"../types\").SnapAlign;\r\n        validator(value: string): boolean;\r\n    };\r\n    transition: {\r\n        default: number | undefined;\r\n        type: NumberConstructor;\r\n    };\r\n    breakpoints: {\r\n        default: import(\"../types\").Breakpoints | undefined;\r\n        type: ObjectConstructor;\r\n    };\r\n    autoplay: {\r\n        default: number | undefined;\r\n        type: NumberConstructor;\r\n    };\r\n    pauseAutoplayOnHover: {\r\n        default: boolean | undefined;\r\n        type: BooleanConstructor;\r\n    };\r\n    modelValue: {\r\n        default: undefined;\r\n        type: NumberConstructor;\r\n    };\r\n    mouseDrag: {\r\n        default: boolean | undefined;\r\n        type: BooleanConstructor;\r\n    };\r\n    touchDrag: {\r\n        default: boolean | undefined;\r\n        type: BooleanConstructor;\r\n    };\r\n    dir: 