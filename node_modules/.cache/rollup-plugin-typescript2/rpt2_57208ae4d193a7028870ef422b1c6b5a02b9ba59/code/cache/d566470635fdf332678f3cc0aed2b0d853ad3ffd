{"code":"export function getMaxSlideIndex({ config, slidesCount }) {\r\n    const { snapAlign, wrapAround, itemsToShow = 1 } = config;\r\n    if (wrapAround) {\r\n        return Math.max(slidesCount - 1, 0);\r\n    }\r\n    let output;\r\n    switch (snapAlign) {\r\n        case 'start':\r\n            output = slidesCount - itemsToShow;\r\n            break;\r\n        case 'end':\r\n            output = slidesCount - 1;\r\n            break;\r\n        case 'center':\r\n        case 'center-odd':\r\n            output = slidesCount - Math.ceil((itemsToShow - 0.5) / 2);\r\n            break;\r\n        case 'center-even':\r\n            output = slidesCount - Math.ceil(itemsToShow / 2);\r\n            break;\r\n        default:\r\n            output = 0;\r\n            break;\r\n    }\r\n    return Math.max(output, 0);\r\n}\r\n","references":["/Users/I560079/Documents/vue-carousel/src/types/index.ts"],"dts":{"name":"/Users/I560079/Documents/vue-carousel/dist/dts/utils/getMaxSlideIndex.d.ts","writeByteOrderMark":false,"text":"import { CarouselConfig } from '../types';\r\ntype Args = {\r\n    config: Partial<CarouselConfig>;\r\n    slidesCount: number;\r\n};\r\nexport declare function getMaxSlideIndex({ config, slidesCount }: Args): number;\r\nexport {};\r\n"}}
